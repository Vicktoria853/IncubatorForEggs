float getTemp(uint8_t pin){
    Vo = analogRead(pin);
    //Serial.print(Vo);
    R2 = R1 * (4095.0 / (float)Vo - 1.0);
    logR2 = log(R2);
    T = (1.0 / (c1 + c2*logR2 + c3*logR2*logR2*logR2));
    T = T - K;
    return T;
}

float calTemp(uint16_t v){
    Vo = v;
    R2 = R1 * (4095.0 / (float)Vo - 1.0);
    logR2 = log(R2);
    T = (1.0 / (c1 + c2*logR2 + c3*logR2*logR2*logR2));
    T = T - K;
    return T;
}
